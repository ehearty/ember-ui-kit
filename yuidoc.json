{
  "name": "ember-ui-kit",
  "description": "The default blueprint for ember-cli addons.",
  "url": "https://github.com/ming-codes/ember-ui-kit",
  "version": "0.10.3",
  "options": {
    "paths": [
      "addon"
    ],
    "external": {
      "data": [
        {
          "base": "http://emberjs.com/api/",
          "json": "http://builds.emberjs.com/tags/v2.8.0/ember-docs.json"
        }
      ]
    },
    "themedir": "node_modules/yuidoc-ember-theme",
    "helpers": [
      "node_modules/yuidoc-ember-theme/helpers/helpers.js"
    ],
    "exclude": "vendor",
    "outdir": "docs",
    "linkNatives": true,
    "quiet": true,
    "parseOnly": false,
    "lint": false
  }
}
